template Core <T, G> : Base <G>  {
	P2(x) <- Q@G(x, _)
	P3(x) <- Q@G(_, x)
	W(x) <- Z@T(x)
	M(x) <- P3(x), !W(x)
	M(x) <- W(x), x>1000
}

