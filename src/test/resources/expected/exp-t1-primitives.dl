p(1).
p(7).
p(255).
p(-42).
p(-34).
p(-66).
p2(x, x + 10) :- p(x).
p3(x, y) :- p2(y, x).
p1(x) :- p2(x + 1, y), p3(x, z), z > y + 10.
p4(x) :- p1(x); p2(x, _).
p5(x) :- ((p1(x), x > 10); p4(x)), !p2(x, _).
p6(x + 10 + z) :- p4(x), p4(z).
p6(t) :- p4(x), p4(t), t = x + 20.
p6(t) :- p4(x), p4(t), t = x * 30.
