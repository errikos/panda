.output f_oo_bar2
.decl p1(v0: number) // int
.decl p2(v0: number, v1: number) // int x int
.decl p3(v0: number, v1: number) // int x int
.decl p4(v0: number) // int
.decl p5(v0: number) // int
.decl p6(v0: number) // int
.output p6
