@Type AP
@Constructor
byVar[x] = y -> string(x), AP(y).
@Constructor
byFld[x, y] = z -> string(x), string(y), AP(z).

new<<byVar[x] = y as AP>>,
Foo(y, x), Baz(x), BarBaz(y) <- x = "abc".

@type AP2 -> AP

new<<byFld[x, y] = z as AP2>>,
Foo2(z), Baz2(y, x) <- x = "x2", y = "f1".

@Plan(val = "1:(1,2)")
Planned(x) <- Foo(_, x), Baz(x).


@Type Shape
@Constructor
byLen[x] = y -> int(x), Shape(y).
@Constructor
byName[x] = y -> string(x), Shape(y).
@Constructor
byPoints[x,y] = z -> int(x), int(y), Shape(z).
